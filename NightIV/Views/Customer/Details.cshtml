@model NightIV.Models.Customer


@{
    ViewBag.Title = "Details";
    Layout = "~/Views/Shared/_Layout.cshtml";
}


@if (Model != null)
{
    <h3>@Model.Name</h3>

    <ul>
        <li>@Model.MemberShipType.Name</li>
        <li>@Model.Birthday.ToString("dd / MMMM / YYYY")</li>
    </ul>
}
else
{
    <h3>There is No Customer!</h3>
}

